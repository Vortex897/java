package main;

public class Main {
	public static boolean ehPar(int num) {
		if (num % 2 == 0) {
			return true;
		} else {
			return false;
		}
	}
	
	public static void main(String args[]) {
		for (int i = 1; i < 51; i++) {
			boolean n = ehPar(i);
			if(n == true) {
				System.out.println(i);
			}
		}
	
	}
}

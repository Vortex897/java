public class Main {
    
    public static void main(String[] args) {
        int i = 1;
        int j = 1;
        int sum = 0;
        
        while (i < 101) {
            System.out.println(i);
            i++;
        }
        
        while (j < 101){
            if (j % 2 == 0) {
                System.out.println(j);
            }
            sum += j;
            j++;
        }
        
        System.out.println(sum);
    }
    
}
